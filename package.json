{
  "name": "openclaw-skill-audiobookshelf",
  "version": "0.0.2",
  "description": "OpenClaw skill for Audiobookshelf with Google Cast support",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "abs": "dist/bin/abs.js"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "dev": "tsc --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src/ tests/",
    "lint:fix": "eslint src/ tests/ --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist/ coverage/",
    "prepack": "pnpm run build"
  },
  "keywords": [
    "openclaw",
    "skill",
    "audiobookshelf",
    "audiobook",
    "google-cast",
    "chromecast"
  ],
  "author": "Troy Kelly",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/troykelly/openclaw-skill-audiobookshelf.git"
  },
  "engines": {
    "node": ">=22"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@types/castv2": "^0.1.3",
    "@types/node": "^22.0.0",
    "@vitest/coverage-v8": "^3.0.0",
    "eslint": "^9.0.0",
    "prettier": "^3.0.0",
    "typescript": "^5.7.0",
    "typescript-eslint": "^8.0.0",
    "vitest": "^3.0.0"
  },
  "dependencies": {
    "bonjour-service": "^1.3.0",
    "castv2-client": "^1.2.0"
  },
  "files": [
    "dist",
    "SKILL.md",
    "README.md",
    "LICENSE"
  ]
}
